<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="ButtonCVTheme" parent="android:Widget.Button">
	  <item name="android:background">@drawable/cvtheme_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ImageButtonCVTheme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/cvtheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerCVTheme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/cvtheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/cvtheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemCVTheme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/cvtheme_btn_radio_holo_light</item>
  </style>
  <style name="TabCVTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/cvtheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextCVTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarCVTheme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/cvtheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/cvtheme_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarCVTheme" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/cvtheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/cvtheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/cvtheme_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="ToggleCVTheme" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/cvtheme_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ListViewCVTheme" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/cvtheme_list_selector_holo_light</item>
  </style>

  <style name="ListViewCVTheme.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/cvtheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemCVTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#000000</item>
  </style>

</resources>