<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ButtonCVTheme" parent="android:Widget.Button">
        <item name="android:background">@drawable/cvtheme_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonCVTheme" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/cvtheme_btn_default_holo_light</item>
    </style>



    <style name="TabCVTheme">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/cvtheme_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextCVTheme">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ListViewCVTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/cvtheme_list_selector_holo_light</item>
    </style>

    <style name="ListViewCVTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/cvtheme_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemCVTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="titleHeader" parent="android:Widget.Holo.Light.TextView">
        <item name="android:drawableBottom">@drawable/section_header</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:layout_marginTop">@dimen/home_margin_top_title</item>
        <item name="android:layout_marginBottom">@dimen/home_margin_bottom_title</item>
        <item name="android:textColor">@color/cvtheme_color</item>
        <item name="android:textSize">14sp</item>
        <!--<item name="android:background">@drawable/list_section_divider_holo_light</item>-->
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">8dip</item>
    </style>

    <style name="textHome" parent="android:Widget.Holo.Light.TextView">
        <item name="android:textColor">#323232</item>
    </style>
</resources>